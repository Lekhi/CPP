#include <iostream>


using namespace std;

void show(int **array_ptr_array){

   for(int i = 0; i < 3; i++){
       for(int j =0; j < 3; j++){
           cout<<array_ptr_array[i][j];
       }
       cout<<endl;
   }
}


int main() {



    int  array1 [3]  {1, 2, 3};
    int  array2 [3]  {4, 5, 6};
    int  array3 [3]  {7, 8, 9};

    int *array1_ptr = array1;
    int *array2_ptr = array2;
    int *array3_ptr = array3;


    int *array_ptr_array [3] {array1_ptr, array2_ptr, array3_ptr};

    show(array_ptr_array);
   
   
   return 0;
}
